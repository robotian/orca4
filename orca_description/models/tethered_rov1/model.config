<!-- So, name this model 'orca4' to avoid a conflict. -->
<!-- See: https://answers.gazebosim.org/question/28549/gz_sim_resource_path-includes-usrshare/ -->

<model>
  <name>tethered_rov1</name>
  <version>4.0</version>
  <sdf version="1.6">model.sdf</sdf>

  <author>
    <name>Myoungkuk Park</name>
    <email>robotian@gmail.com</email>
  </author>

  <maintainer email="robotian@gmail.com">Myoungkuk Park</maintainer>

  <description>
    BlueROV2 with tether cable model based on ORCA4 repo
  </description>
</model>
